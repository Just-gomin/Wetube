{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with VanillaJS and NodeJS",
  "main": "index.js",
  "engines": {
    "node": "12.16.1",
    "npm": "6.14.5"
  },
  "scripts": {
    "dev:assets": "cd src && webpack -w",
    "dev:server": "nodemon --exec babel-node src\\init.js --delay 2 --ignore './assets\\scss' --ignore 'static'",
    "build:assets": "cd src && webpack",
    "build:server": "babel src --out-dir build --ignore \"src\\assets\",\"src\\static\",\"src\\webpack.config.js\"",
    "copyAll": "shx cp -r src/static build && shx cp -r src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "shx rm -rf build",
    "start": "node build/init.js",
    "prestart": "npm run build",
    "tunnel": "lt -h http://serverless.social -p 4000"
  },
  "author": "Kangmin AN",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.3",
    "autoprefixer": "^9.8.4",
    "aws-sdk": "^2.716.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.2",
    "helmet": "^3.23.2",
    "mongoose": "^5.9.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-kakao": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.0",
    "sass-loader": "^9.0.1",
    "shx": "^0.3.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
